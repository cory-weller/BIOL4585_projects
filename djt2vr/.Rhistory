install.packages("ggplot2")
library(ggplot2)
library(data.table)
crust <- fread('FE_datfile_TTolerance.csv?sequence=1')
crust[,Treatment := factor(Treatment, levels = c("hypoxia", "normoxia", "hyperoxia"))]
Q1 <- ggplot(crust, aes(x = Treatment, y = CTmax1, color = status)) + geom_boxplot() + xlab("Oxygen Condition") + ylab("Critical Temperature") + ggtitle("Relationship Between Critical Temperature and Oxygen Condition")
ggsave('Relationship_Between_Critical_Temperature_and_Oxygen_Condition.png', Q1)
