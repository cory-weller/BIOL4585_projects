install.packages("ggplot2")
library(ggplot2)
library(data.table)
crust <- fread('FE_datfile_TTolerance.csv?sequence=1')
crust[,Treatment := factor(Treatment, levels = c("hypoxia", "normoxia", "hyperoxia"))]
ggplot(crust, aes(x = Treatment, y = CTmax1, color = status)) + geom_boxplot() + xlab("Oxygen Condition") + ylab("Critical Temperature") + ggtitle("Relationship Between Critical Temperature and Oxygen Condition")
Q1 <- ggplot(crust, aes(x = Treatment, y = CTmax1, color = status)) + geom_boxplot() + xlab("Oxygen Condition") + ylab("Critical Temperature") + ggtitle("Relationship Between Critical Temperature and Oxygen Condition")
ggsave('Relationship_Between_Critical_Temperature_and_Oxygen_Condition.png', Q1)
crust2 <- melt(crust, measure.vars = c("CTmax3", "CTmax2", "CTmax1"))
ggplot(crust2, aes(x = variable, y = value, color = variable)) + geom_boxplot() + facet_wrap(vars(habitat)) + xlab("Habitat Type") + ylab("Critical Temperature") + ggtitle("Relationship Between Critical Temperature and Habitat Type")
Q2 <- ggplot(crust2, aes(x = variable, y = value, color = variable)) + geom_boxplot() + facet_wrap(vars(habitat)) + xlab("Habitat Type") + ylab("Critical Temperature") + ggtitle("Relationship Between Critical Temperature and Habitat Type")
ggsave('Relationship_Between_Critical_Temperature_and_Habitat_Type.png', Q2)
Q3 <- ggplot(crust, aes(CTmax1, stat(count), fill = species.name, color = spec> s.name)) + geom_density(alpha = 0.1) + xlab("Critical Temperature Maximum 1") > ylab("Frequency") + ggtitle("Critical Temperature by Species")
ggsave('Critical Temperature by Species.png', Q3)
